{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ShopItem",
    "definitions": {
        "ShopItem": {
            "properties": {
                "shopId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"shopId\""
                },
                "buyCnt": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buyCnt\"  ! 已购买次数"
                },
                "lastBuyTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastBuyTime\"  ! 未刷新前购买时间戳"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Shop Item"
        }
    }
}