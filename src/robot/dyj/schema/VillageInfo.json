{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/VillageInfo",
    "definitions": {
        "VillageInfo": {
            "properties": {
                "unlockVillageIdList": {
                    "items": {
                        "enum": [
                            "InvalidVillage",
                            "One",
                            "Two"
                        ]
                    },
                    "type": "array",
                    "title": "Village Id"
                },
                "buildingList": {
                    "items": {
                        "$ref": "#/definitions/model.Building"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"buildingList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Village Info"
        },
        "model.Building": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "model": {
                    "$ref": "#/definitions/model.Model",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"model\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "buildStartTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buildStartTime\"  ! 开始建造时间"
                },
                "status": {
                    "enum": [
                        "BS_Build_Start",
                        "BS_Repair_Start",
                        "BS_Remove_Start",
                        "BS_End"
                    ],
                    "type": "string",
                    "title": "Building Status"
                },
                "petAssignInfo": {
                    "$ref": "#/definitions/model.PetAssignInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"petAssignInfo\""
                },
                "scienceInfo": {
                    "$ref": "#/definitions/model.ScienceInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"scienceInfo\""
                },
                "npcBufferList": {
                    "items": {
                        "$ref": "#/definitions/model.PetBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"npcBufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Building"
        },
        "model.Model": {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"id\""
                },
                "direction": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"direction\""
                },
                "x": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"x\""
                },
                "y": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"y\""
                },
                "p": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"p\""
                },
                "params": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"params\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Model"
        },
        "model.PetAssignInfo": {
            "properties": {
                "petUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"petUid\""
                },
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.PetBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Assign Info"
        },
        "model.PetBuffer": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"uid\""
                },
                "bufferId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferId\""
                },
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "bufferTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferTime\"  ! 生效结束时间 0表示永久"
                },
                "bufferSrc": {
                    "enum": [
                        "EBS_Invalid",
                        "EBS_Attr",
                        "EBS_Skill"
                    ],
                    "type": "string",
                    "title": "E Buffer Src"
                },
                "ApplyTargetParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"ApplyTargetParams\""
                },
                "EffectiveConditionID": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveConditionID\""
                },
                "EffectiveParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveParams\""
                },
                "EffectiveProbability": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveProbability\""
                },
                "EffectParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectParams\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Buffer"
        },
        "model.ScienceBuffer": {
            "properties": {
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "scienceBuffType": {
                    "enum": [
                        "ESBT_Invalid",
                        "ESBT_ReduceCD",
                        "ESBT_AddOutPut",
                        "ESBT_LandBaseSpeed",
                        "ESBT_LandOutPut",
                        "ESBT_ExpandWarehouseCap"
                    ],
                    "type": "string",
                    "title": "E Science Buff Type"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Science Buffer"
        },
        "model.ScienceInfo": {
            "properties": {
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.ScienceBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Science Info"
        }
    }
}