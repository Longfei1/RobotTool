{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/OrderItem",
    "definitions": {
        "OrderItem": {
            "properties": {
                "orderGlobalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"orderGlobalItemId\""
                },
                "fromPoolId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"fromPoolId\"  ! 来源于哪个池子,用于限量"
                },
                "costItemList": {
                    "items": {
                        "$ref": "#/definitions/model.BaseItemInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"costItemList\""
                },
                "refreshTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"refreshTime\"  ! 下次可刷新时间"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Order Item"
        },
        "model.BaseItemInfo": {
            "properties": {
                "itemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemId\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Base Item Info"
        }
    }
}