{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ActDataTaskPlan",
    "definitions": {
        "ActDataTaskPlan": {
            "properties": {
                "exp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"exp\""
                },
                "extraUnlock": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"extraUnlock\""
                },
                "awardData": {
                    "items": {
                        "$ref": "#/definitions/model.ActDataTaskPlanAwardItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"awardData\"  奖励领取数据"
                },
                "questData": {
                    "items": {
                        "$ref": "#/definitions/model.ActDataTaskPlanQuestItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"questData\"  任务领取数据"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Act Data Task Plan"
        },
        "model.ActDataTaskPlanAwardItem": {
            "properties": {
                "cfgId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"cfgId\""
                },
                "hasGot": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"hasGot\""
                },
                "extraHasGot": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"extraHasGot\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Act Data Task Plan Award Item"
        },
        "model.ActDataTaskPlanQuestItem": {
            "properties": {
                "cfgId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"cfgId\""
                },
                "hasGot": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"hasGot\""
                },
                "lastGotTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastGotTime\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Act Data Task Plan Quest Item"
        }
    }
}