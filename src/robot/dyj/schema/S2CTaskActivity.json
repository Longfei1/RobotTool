{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/S2CTaskActivity",
    "definitions": {
        "S2CTaskActivity": {
            "properties": {
                "uiId": {
                    "type": "integer"
                },
                "taskActivityGetRsp": {
