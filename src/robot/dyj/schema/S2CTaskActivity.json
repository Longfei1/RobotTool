                                    {
                                        "required": [
                                            "taskActivityRewardRsp"
                                        ]
                                    }
                                ]
                            }
                        },
                        {
                            "required": [
                                "taskActivityGetRsp"
                            ]
                        },
                        {
                            "required": [
                                "taskActivityRewardRsp"
                            ]
                        }
                    ]
                }
            ],
