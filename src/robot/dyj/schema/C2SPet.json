{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/C2SPet",
    "definitions": {
        "C2SPet": {
            "properties": {
                "uiId": {
                    "type": "integer"
                },
                "petUid": {
                    "type": "string"
                },
                "operaType": {
                    "enum": [
                        "EPO_INVALID",
                        "EPO_FETCH",
                        "EPO_UPGRADE",
                        "EPO_ASSIGN",
                        "EPO_UN_ASSIGN",
                        "EPO_RESET",
                        "EPO_SKILL_UPGRADE",
                        "EPO_SKILL_CHANGE",
                        "EPO_RELEASE"
                    ],
                    "type": "string",
                    "title": "E Pet Opera"
                },
                "petUpgrade": {
                    "$ref": "#/definitions/msg.PetUpgrade",
                    "additionalProperties": true
                },
                "petAssign": {
                    "$ref": "#/definitions/msg.PetAssign",
                    "additionalProperties": true
                },
                "petUnAssign": {
                    "$ref": "#/definitions/msg.PetUnAssign",
                    "additionalProperties": true
                },
                "petSkillUpgrade": {
                    "$ref": "#/definitions/msg.PetSkillUpgrade",
                    "additionalProperties": true,
                    "description": "技能升级"
                },
                "petSkillChange": {
                    "$ref": "#/definitions/msg.PetSkillChange",
                    "additionalProperties": true,
                    "description": "技能重置"
                },
                "petSkillChangeConfirm": {
                    "$ref": "#/definitions/msg.PetSkillChangeConfirm",
                    "additionalProperties": true,
                    "description": "确认技能重置"
                },
                "petRename": {
                    "$ref": "#/definitions/msg.PetRename",
                    "additionalProperties": true
                },
                "petRelease": {
                    "$ref": "#/definitions/msg.PetRelease",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "allOf": [
                {
                    "oneOf": [
                        {
                            "not": {
                                "anyOf": [
                                    {
                                        "required": [
                                            "petUpgrade"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petAssign"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petUnAssign"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petSkillUpgrade"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petSkillChange"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petSkillChangeConfirm"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petRename"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "petRelease"
                                        ]
                                    }
                                ]
                            }
                        },
                        {
                            "required": [
                                "petUpgrade"
                            ]
                        },
                        {
                            "required": [
                                "petAssign"
                            ]
                        },
                        {
                            "required": [
                                "petUnAssign"
                            ]
                        },
                        {
                            "required": [
                                "petSkillUpgrade"
                            ]
                        },
                        {
                            "required": [
                                "petSkillChange"
                            ]
                        },
                        {
                            "required": [
                                "petSkillChangeConfirm"
                            ]
                        },
                        {
                            "required": [
                                "petRename"
                            ]
                        },
                        {
                            "required": [
                                "petRelease"
                            ]
                        }
                    ]
                }
            ],
            "title": "C 2 S Pet"
        },
        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        },
        "msg.PetAssign": {
            "properties": {
                "workBuildingUid": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Assign"
        },
        "msg.PetRelease": {
            "properties": {
                "petUid": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Release",
            "description": "重命名"
        },
        "msg.PetRename": {
            "properties": {
                "name": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Rename",
            "description": "重命名"
        },
        "msg.PetSkillChange": {
            "properties": {
                "skillIndex": {
                    "type": "integer"
                },
                "skillGlobalItemId": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Skill Change",
            "description": "技能重置"
        },
        "msg.PetSkillChangeConfirm": {
            "properties": {
                "skillIndex": {
                    "type": "integer"
                },
                "skillGlobalItemId": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Skill Change Confirm",
            "description": "确定技能重置"
        },
        "msg.PetSkillUpgrade": {
            "properties": {
                "skillIndex": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Skill Upgrade",
            "description": "技能升级"
        },
        "msg.PetUnAssign": {
            "properties": {
                "workBuildingUid": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Un Assign"
        },
        "msg.PetUpgrade": {
            "properties": {
                "propList": {
                    "items": {
                        "$ref": "#/definitions/model.ItemInfo"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Upgrade"
        }
    }
}