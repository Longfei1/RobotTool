{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/S2COpenBoxRsp",
    "definitions": {
        "S2COpenBoxRsp": {
            "properties": {
                "uiId": {
                    "type": "integer"
                },
                "errCode": {
                    "enum": [
                        "OK",
                        "UNKNOWN_ERROR",
                        "NOT_AUTH",
                        "TOKEN_INVALID",
                        "PLAYER_NOT_FOUND",
                        "REDIRECT",
                        "IDLE_TIMEOUT",
                        "DUP_LOGIN",
                        "PARAM_ERROR",
                        "SERVER_CLOSED",
                        "SYSTEM_ERR",
                        "SCORE_LIMIT",
                        "BUILDING_NOT_FOUND",
                        "BUILDING_STATUS_STARTING",
                        "BUILDING_CAN_NOT_PUT",
                        "BUILDING_CAN_NOT_REMOVE",
                        "BUILDING_CAN_NOT_ADD",
                        "BUILDING_COMFORT_LIMIT",
                        "BUILDING_USER_LVL_LIMIT",
                        "BUILDING_NUM_LIMIT",
                        "BUILDING_COMFORT_ENOUGH",
                        "PLANT_ATTACH_BUILDING_NOT_FOUND",
                        "PLANT_ATTACH_BUILDING_TYPE_ERR",
                        "PLANT_ATTACH_BUILDING_HAVE_ATTACHED",
                        "PLANT_NOT_FOUND",
                        "PLANT_NOT_MATURE",
                        "PLANT_PRUNE_STEP_ERR",
                        "PLANT_PRUNE_TIME_ZERO",
                        "PLANT_GRAFT_STEP_ERR",
                        "PLANT_NOT_FOUND_CAN_DE_BUFFER",
                        "PLANT_ALREADY_DE_BUFFER",
                        "ITEM_NOT_FOUND",
                        "PROP_NOT_FOUND",
                        "PROP_NOT_ENOUGH",
                        "ORDER_NOT_FOUND",
                        "ORDER_SUBMIT_CD",
                        "WAREHOUSE_CAPACITY_ERR",
                        "PRODUCT_NOT_FOUND",
                        "PRODUCT_NOT_END",
                        "CHANGE_NICK_CD_ERR",
                        "SHOP_EXCEED_LIMIT",
                        "TASK_REWARDED",
                        "TASK_COND_NOT_COM",
                        "TASK_PRE_NOT_COM",
                        "FORMULA_NOT_MATCH",
                        "DELIVERY_GOODS_ALREADY_LOAD",
                        "DELIVERY_CARGO_FIN",
                        "DELIVERY_CARGO_NOT_FIN",
                        "DELIVERY_GOODS_ALREADY_REWARD",
                        "DELIVERY_CARGO_STARTED",
                        "XIAO_XIAO_LE_MATCH_TIME_OUT",
                        "XIAO_XIAO_LE_GAME_END",
                        "ACTIVITY_LVL_NOT_MET",
                        "ACTIVITY_NOT_IN_TIME",
                        "EXCHANGE_CODE_INVALID",
                        "EXCHANGE_CODE_EXPIRE",
                        "EXCHANGE_CODE_TIME_NOT_OPEN",
                        "EXCHANGE_CODE_USED",
                        "EXCHANGE_CODE_BATCH_COUNT_LIMIT",
                        "SMALL_GAME_NOT_OPEN",
                        "SMALL_GAME_IN_GAME",
                        "SMALL_GAME_NOT_IN_GAME",
                        "SMALL_GAME_OVER"
                    ],
                    "type": "string",
                    "title": "STATUS",
                    "description": "protoc --proto_path=./pb --go_out=./pbgo --go_opt=paths=source_relative ./pb/msg.proto错误码"
                },
                "openOperaRsp": {
                    "$ref": "#/definitions/msg.OpenOperaRsp",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "allOf": [
                {
                    "oneOf": [
                        {
                            "not": {
                                "anyOf": [
                                    {
                                        "required": [
                                            "openOperaRsp"
                                        ]
                                    }
                                ]
                            }
                        },
                        {
                            "required": [
                                "openOperaRsp"
                            ]
                        }
                    ]
                }
            ],
            "title": "S 2 C Open Box Rsp"
        },
        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        },
        "model.PetInfo": {
            "properties": {
                "petUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"petUid\""
                },
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\"  ! 全局id"
                },
                "birthTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"birthTime\"  ! 生效时间"
                },
                "name": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"name\""
                },
                "exp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"exp\"  ! 放大100倍"
                },
                "awakeLevel": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"awakeLevel\"  ! 觉醒等级跟突破是强关联的, 突破后觉醒等级才会提升"
                },
                "attrList": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"attrList\""
                },
                "talentList": {
                    "items": {
                        "$ref": "#/definitions/model.TalentInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"talentList\""
                },
                "skillList": {
                    "items": {
                        "$ref": "#/definitions/model.SkillInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"skillList\""
                },
                "buildUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"buildUid\"  ! 依附的建唯一id"
                },
                "buildGlobalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buildGlobalItemId\"  ! 依附建筑的全局id"
                },
                "dispatchPointId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"dispatchPointId\"  ! 派遣点id"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Info"
        },
        "model.SkillInfo": {
            "properties": {
                "skillId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"skillId\""
                },
                "lvl": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lvl\""
                },
                "tempSkillId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"tempSkillId\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Skill Info"
        },
        "model.TalentInfo": {
            "properties": {
                "talentId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"talentId\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Talent Info",
            "description": "天赋等级是觉醒等级+1"
        },
        "msg.ItemChangeInfo": {
            "properties": {
                "ItemChange": {
                    "enum": [
                        "ItemAll",
                        "UpdateTo"
                    ],
                    "type": "string",
                    "title": "Item Change"
                },
                "itemList": {
                    "items": {
                        "$ref": "#/definitions/model.ItemInfo"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Change Info"
        },
        "msg.OpenOperaRsp": {
            "properties": {
                "itemChangeInfo": {
                    "items": {
                        "$ref": "#/definitions/msg.ItemChangeInfo"
                    },
                    "type": "array"
                },
                "itemList": {
                    "items": {
                        "$ref": "#/definitions/model.ItemInfo"
                    },
                    "type": "array",
                    "description": "用于组装common reward"
                },
                "petInfoList": {
                    "items": {
                        "$ref": "#/definitions/model.PetInfo"
                    },
                    "type": "array",
                    "description": "用于组装common reward"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Open Opera Rsp"
        }
    }
}