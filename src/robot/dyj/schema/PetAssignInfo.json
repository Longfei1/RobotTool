{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/PetAssignInfo",
    "definitions": {
        "PetAssignInfo": {
            "properties": {
                "petUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"petUid\""
                },
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.PetBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Assign Info"
        },
        "model.PetBuffer": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"uid\""
                },
                "bufferId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferId\""
                },
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "bufferTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferTime\"  ! 生效结束时间 0表示永久"
                },
                "bufferSrc": {
                    "enum": [
                        "EBS_Invalid",
                        "EBS_Attr",
                        "EBS_Skill"
                    ],
                    "type": "string",
                    "title": "E Buffer Src"
                },
                "ApplyTargetParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"ApplyTargetParams\""
                },
                "EffectiveConditionID": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveConditionID\""
                },
                "EffectiveParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveParams\""
                },
                "EffectiveProbability": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveProbability\""
                },
                "EffectParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectParams\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Buffer"
        }
    }
}