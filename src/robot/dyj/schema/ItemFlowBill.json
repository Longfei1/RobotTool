{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ItemFlowBill",
    "definitions": {
        "ItemFlowBill": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"uid\""
                },
                "timeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"timeStamp\"  道具变化时间"
                },
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "isSub": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"isSub\"  0增加 1减少"
                },
                "count": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"count\""
                },
                "afterCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"afterCount\""
                },
                "reason": {
                    "enum": [
                        "EPOI_Invalid",
                        "EPOI_Act_TaskPlan",
                        "EPOI_Act_LvlUpgrade"
                    ],
                    "type": "string",
                    "title": "E User Oper Id",
                    "description": "玩家操作, 账单用"
                },
                "subReason": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"subReason\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Flow Bill",
            "description": "道具流水账单"
        }
    }
}