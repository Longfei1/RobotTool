        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        },
        "model.PetAssignInfo": {
            "properties": {
                "petUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"petUid\""
                },
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.PetBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Assign Info"
        },
        "model.PetBuffer": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"uid\""
                },
                "bufferId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferId\""
                },
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "bufferTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferTime\"  ! 生效结束时间 0表示永久"
                },
                "bufferSrc": {
                    "enum": [
                        "EBS_Invalid",
                        "EBS_Attr",
                        "EBS_Skill"
