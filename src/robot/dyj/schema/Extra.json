                "EffectParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectParams\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Buffer"
        },
        "model.Task": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "index为uid和taskId，唯一id系统生成 @inject_tag: bson:\"uid\""
