{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/C2SNPCInfo",
    "definitions": {
        "C2SNPCInfo": {
            "properties": {
                "uiId": {
                    "type": "integer"
                },
                "getNPCInfo": {
                    "$ref": "#/definitions/msg.GetNPCInfo",
                    "additionalProperties": true
                },
                "inviteNPC": {
                    "$ref": "#/definitions/msg.InviteNPC",
                    "additionalProperties": true
                },
                "feedNPC": {
                    "$ref": "#/definitions/msg.FeedNPC",
                    "additionalProperties": true
                },
                "talkNPC": {
                    "$ref": "#/definitions/msg.TalkNPC",
                    "additionalProperties": true
                },
                "takeReward": {
                    "$ref": "#/definitions/msg.TakeReward",
                    "additionalProperties": true
                },
                "firstTalk": {
                    "$ref": "#/definitions/msg.FirstTalk",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "allOf": [
                {
                    "oneOf": [
                        {
                            "not": {
                                "anyOf": [
                                    {
                                        "required": [
                                            "getNPCInfo"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "inviteNPC"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "feedNPC"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "talkNPC"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "takeReward"
                                        ]
                                    },
                                    {
                                        "required": [
                                            "firstTalk"
                                        ]
                                    }
                                ]
                            }
                        },
                        {
                            "required": [
                                "getNPCInfo"
                            ]
                        },
                        {
                            "required": [
                                "inviteNPC"
                            ]
                        },
                        {
                            "required": [
                                "feedNPC"
                            ]
                        },
                        {
                            "required": [
                                "talkNPC"
                            ]
                        },
                        {
                            "required": [
                                "takeReward"
                            ]
                        },
                        {
                            "required": [
                                "firstTalk"
                            ]
                        }
                    ]
                }
            ],
            "title": "C 2 SNPC Info"
        },
        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        },
        "msg.FeedNPC": {
            "properties": {
                "npcId": {
                    "type": "integer"
                },
                "itemInfo": {
                    "$ref": "#/definitions/model.ItemInfo",
                    "additionalProperties": true
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Feed NPC",
            "description": "! 投喂NPC喜好礼物"
        },
        "msg.FirstTalk": {
            "properties": {
                "npcId": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "First Talk",
            "description": "! 进行初见任务"
        },
        "msg.GetNPCInfo": {
            "additionalProperties": true,
            "type": "object",
            "title": "Get NPC Info",
            "description": "! 拉取npc信息"
        },
        "msg.InviteNPC": {
            "properties": {
                "npcId": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Invite NPC",
            "description": "! 邀请npc"
        },
        "msg.TakeReward": {
            "properties": {
                "npcId": {
                    "type": "integer"
                },
                "itemIndex": {
                    "type": "integer",
                    "description": "领取物品的索引 默认从0开始"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Take Reward",
            "description": "! 领取手信"
        },
        "msg.TalkNPC": {
            "properties": {
                "npcId": {
                    "type": "integer"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Talk NPC",
            "description": "谈天说地"
        }
    }
}