{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/EventStats",
    "definitions": {
        "EventStats": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "eventStatsList": {
                    "items": {
                        "$ref": "#/definitions/model.EventStatsItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"eventStatsList\""
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Event Stats"
        },
        "model.EventStatsItem": {
            "properties": {
                "statsId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"statsId\""
                },
                "statsCnt": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"statsCnt\"  ! 统计数量"
                },
                "updateTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"updateTime\"  ! 统计更新时间"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Event Stats Item"
        }
    }
}