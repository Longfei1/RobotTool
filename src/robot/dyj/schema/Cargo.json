{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Cargo",
    "definitions": {
        "Cargo": {
            "properties": {
                "goodsList": {
                    "items": {
                        "$ref": "#/definitions/model.Goods"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"goodsList\"  ! 随机3到4个物品"
                },
                "finRewardList": {
                    "items": {
                        "$ref": "#/definitions/model.RewardInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"finRewardList\"  ! 完成后的奖励列表"
                },
                "deliveryDis": {
                    "$ref": "#/definitions/model.DeliveryDis",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"deliveryDis\""
                },
                "startFlag": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"startFlag\"  ! 是否出发  1出发"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Cargo"
        },
        "model.CommonItem": {
            "properties": {
                "itemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemId\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Common Item"
        },
        "model.DeliveryDis": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"name\""
                },
                "startTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"startTime\""
                },
                "finTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"finTime\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Delivery Dis"
        },
        "model.Goods": {
            "properties": {
                "goodsItem": {
                    "$ref": "#/definitions/model.CommonItem",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"goodsItem\""
                },
                "loadRewardList": {
                    "items": {
                        "$ref": "#/definitions/model.CommonItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"loadRewardList\"  ! 装车奖励信息"
                },
                "loadFlag": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"loadFlag\"  ! 是否装修货物 1代表装填, 用于单个物品装填后发奖励"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Goods"
        },
        "model.RewardInfo": {
            "properties": {
                "finRewardItem": {
                    "$ref": "#/definitions/model.CommonItem",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"finRewardItem\""
                },
                "rewardedFlag": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"rewardedFlag\"  ! 是否领取奖励 0没领取 1已领取"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Reward Info"
        }
    }
}