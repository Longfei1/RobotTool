{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/S2CNPCInfo",
    "definitions": {
        "S2CNPCInfo": {
            "properties": {
                "errCode": {
                    "enum": [
                        "OK",
                        "UNKNOWN_ERROR",
                        "NOT_AUTH",
                        "TOKEN_INVALID",
                        "PLAYER_NOT_FOUND",
                        "REDIRECT",
                        "IDLE_TIMEOUT",
                        "DUP_LOGIN",
                        "PARAM_ERROR",
                        "SERVER_CLOSED",
                        "SYSTEM_ERR",
                        "SCORE_LIMIT",
                        "BUILDING_NOT_FOUND",
                        "BUILDING_STATUS_STARTING",
                        "BUILDING_CAN_NOT_PUT",
                        "BUILDING_CAN_NOT_REMOVE",
                        "BUILDING_CAN_NOT_ADD",
                        "BUILDING_COMFORT_LIMIT",
                        "BUILDING_USER_LVL_LIMIT",
                        "BUILDING_NUM_LIMIT",
                        "BUILDING_COMFORT_ENOUGH",
                        "PLANT_ATTACH_BUILDING_NOT_FOUND",
                        "PLANT_ATTACH_BUILDING_TYPE_ERR",
                        "PLANT_ATTACH_BUILDING_HAVE_ATTACHED",
                        "PLANT_NOT_FOUND",
                        "PLANT_NOT_MATURE",
                        "PLANT_PRUNE_STEP_ERR",
                        "PLANT_PRUNE_TIME_ZERO",
                        "PLANT_GRAFT_STEP_ERR",
                        "PLANT_NOT_FOUND_CAN_DE_BUFFER",
                        "PLANT_ALREADY_DE_BUFFER",
                        "ITEM_NOT_FOUND",
                        "PROP_NOT_FOUND",
                        "PROP_NOT_ENOUGH",
                        "ORDER_NOT_FOUND",
                        "ORDER_SUBMIT_CD",
                        "WAREHOUSE_CAPACITY_ERR",
                        "PRODUCT_NOT_FOUND",
                        "PRODUCT_NOT_END",
                        "CHANGE_NICK_CD_ERR",
                        "SHOP_EXCEED_LIMIT",
                        "TASK_REWARDED",
                        "TASK_COND_NOT_COM",
                        "TASK_PRE_NOT_COM",
                        "FORMULA_NOT_MATCH",
                        "DELIVERY_GOODS_ALREADY_LOAD",
                        "DELIVERY_CARGO_FIN",
                        "DELIVERY_CARGO_NOT_FIN",
                        "DELIVERY_GOODS_ALREADY_REWARD",
                        "DELIVERY_CARGO_STARTED",
                        "XIAO_XIAO_LE_MATCH_TIME_OUT",
                        "XIAO_XIAO_LE_GAME_END",
                        "ACTIVITY_LVL_NOT_MET",
                        "ACTIVITY_NOT_IN_TIME",
                        "EXCHANGE_CODE_INVALID",
                        "EXCHANGE_CODE_EXPIRE",
                        "EXCHANGE_CODE_TIME_NOT_OPEN",
                        "EXCHANGE_CODE_USED",
                        "EXCHANGE_CODE_BATCH_COUNT_LIMIT"
                    ],
                    "type": "string",
                    "title": "STATUS",
                    "description": "protoc --proto_path=./pb --go_out=./pbgo --go_opt=paths=source_relative ./pb/msg.proto错误码"
                },
                "uiId": {
                    "type": "integer"
                },
                "npc": {
                    "$ref": "#/definitions/model.Npc",
                    "additionalProperties": true,
                    "description": "已解锁npc列表"
                },
                "talkId": {
                    "type": "integer",
                    "description": "谈天说地返回的id"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "S 2 CNPC Info"
        },
        "model.NPCDayInfo": {
            "properties": {
                "lastInvitedTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastInvitedTime\"  上次被邀请时间"
                },
                "feedTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"feedTime\"  被投喂次数"
                },
                "talkFlag": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"talkFlag\"  今日是否谈天说地的标志位"
                },
                "operaRecordList": {
                    "items": {
                        "$ref": "#/definitions/model.NPCOperaRecord"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"operaRecordList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "NPC Day Info",
            "description": "npc每天需要刷新的信息"
        },
        "model.NPCInfo": {
            "properties": {
                "npcId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"npcId\""
                },
                "favorAbility": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"favorAbility\"  好感度"
                },
                "npcDayInfo": {
                    "$ref": "#/definitions/model.NPCDayInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"npcDayInfo\""
                },
                "rewardedTokenList": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"rewardedTokenList\"  已领取信物奖励的信物id列表"
                },
                "firstTalk": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"firstTalk\"  是否进行初见任务"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "NPC Info"
        },
        "model.NPCOperaRecord": {
            "properties": {
                "time": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"time\"  操作时间"
                },
                "operaId": {
                    "enum": [
                        "NOI_Invalid",
                        "NOI_Invite",
                        "NOI_Feed"
                    ],
                    "type": "string",
                    "title": "Npc Opera Id"
                },
                "operaValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"operaValue\"  操作对应的值，比如增加好感度等"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "NPC Opera Record",
            "description": "npc每天的操作记录"
        },
        "model.Npc": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "npcList": {
                    "items": {
                        "$ref": "#/definitions/model.NPCInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"npcList\""
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Npc"
        }
    }
}