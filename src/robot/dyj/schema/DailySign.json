{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/DailySign",
    "definitions": {
        "DailySign": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "lastActDataClearTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastActDataClearTime\""
                },
                "dailySignList": {
                    "items": {
                        "enum": [
                            "EDSS_None",
                            "EDSS_Signed",
                            "EDSS_CanSign",
                            "EDSS_ReSigned",
                            "EDSS_CanReSign"
                        ]
                    },
                    "type": "array",
                    "title": "E Daily Sign State"
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Daily Sign"
        }
    }
}