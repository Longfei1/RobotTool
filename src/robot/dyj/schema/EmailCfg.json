{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/EmailCfg",
    "definitions": {
        "EmailCfg": {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\"  邮件id"
                },
                "effectStartTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"esTime\"  生效开始时间"
                },
                "effectEndTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"eeTime\"  生效结束时间"
                },
                "createTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"cTime\"  创建时间"
                },
                "type": {
                    "enum": [
                        "EMAIL_SYSTEM"
                    ],
                    "type": "string",
                    "title": "E Email Type"
                },
                "title": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"title\"  邮件标题"
                },
                "content": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"content\"  邮件内容"
                },
                "awards": {
                    "items": {
                        "$ref": "#/definitions/model.ItemInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"awards\"  邮件奖励"
                },
                "userLvl": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"uLvl\"  用户等级限制"
                },
                "validTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"vTime\"  有效时间(秒)"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Email Cfg"
        },
        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        }
    }
}