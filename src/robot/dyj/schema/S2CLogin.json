{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/S2CLogin",
    "definitions": {
        "S2CLogin": {
            "properties": {
                "errCode": {
                    "enum": [
                        "OK",
                        "UNKNOWN_ERROR",
                        "NOT_AUTH",
                        "TOKEN_INVALID",
                        "PLAYER_NOT_FOUND",
                        "REDIRECT",
                        "IDLE_TIMEOUT",
                        "DUP_LOGIN",
                        "PARAM_ERROR",
                        "SERVER_CLOSED",
                        "SYSTEM_ERR",
                        "SCORE_LIMIT",
                        "BUILDING_NOT_FOUND",
                        "BUILDING_STATUS_STARTING",
                        "BUILDING_CAN_NOT_PUT",
                        "BUILDING_CAN_NOT_REMOVE",
                        "BUILDING_CAN_NOT_ADD",
                        "BUILDING_COMFORT_LIMIT",
                        "BUILDING_USER_LVL_LIMIT",
                        "BUILDING_NUM_LIMIT",
                        "BUILDING_COMFORT_ENOUGH",
                        "PLANT_ATTACH_BUILDING_NOT_FOUND",
                        "PLANT_ATTACH_BUILDING_TYPE_ERR",
                        "PLANT_ATTACH_BUILDING_HAVE_ATTACHED",
                        "PLANT_NOT_FOUND",
                        "PLANT_NOT_MATURE",
                        "PLANT_PRUNE_STEP_ERR",
                        "PLANT_PRUNE_TIME_ZERO",
                        "PLANT_GRAFT_STEP_ERR",
                        "PLANT_NOT_FOUND_CAN_DE_BUFFER",
                        "PLANT_ALREADY_DE_BUFFER",
                        "ITEM_NOT_FOUND",
                        "PROP_NOT_FOUND",
                        "PROP_NOT_ENOUGH",
                        "ORDER_NOT_FOUND",
                        "ORDER_SUBMIT_CD",
                        "WAREHOUSE_CAPACITY_ERR",
                        "PRODUCT_NOT_FOUND",
                        "PRODUCT_NOT_END",
                        "CHANGE_NICK_CD_ERR",
                        "SHOP_EXCEED_LIMIT",
                        "TASK_REWARDED",
                        "TASK_COND_NOT_COM",
                        "TASK_PRE_NOT_COM",
                        "FORMULA_NOT_MATCH",
                        "DELIVERY_GOODS_ALREADY_LOAD",
                        "DELIVERY_CARGO_FIN",
                        "DELIVERY_CARGO_NOT_FIN",
                        "DELIVERY_GOODS_ALREADY_REWARD",
                        "DELIVERY_CARGO_STARTED",
                        "XIAO_XIAO_LE_MATCH_TIME_OUT",
                        "XIAO_XIAO_LE_GAME_END",
                        "ACTIVITY_LVL_NOT_MET",
                        "ACTIVITY_NOT_IN_TIME",
                        "EXCHANGE_CODE_INVALID",
                        "EXCHANGE_CODE_EXPIRE",
                        "EXCHANGE_CODE_TIME_NOT_OPEN",
                        "EXCHANGE_CODE_USED",
                        "EXCHANGE_CODE_BATCH_COUNT_LIMIT"
                    ],
                    "type": "string",
                    "title": "STATUS",
                    "description": "protoc --proto_path=./pb --go_out=./pbgo --go_opt=paths=source_relative ./pb/msg.proto错误码"
                },
                "userBase": {
                    "$ref": "#/definitions/model.UserBase",
                    "additionalProperties": true
                },
                "world": {
                    "$ref": "#/definitions/model.World",
                    "additionalProperties": true
                },
                "plantList": {
                    "items": {
                        "$ref": "#/definitions/model.PlantInfo"
                    },
                    "type": "array"
                },
                "SvrTime": {
                    "type": "integer"
                },
                "lvl": {
                    "type": "integer"
                },
                "firstLogin": {
                    "type": "boolean"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "S 2 C Login"
        },
        "model.Building": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "model": {
                    "$ref": "#/definitions/model.Model",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"model\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "buildStartTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buildStartTime\"  ! 开始建造时间"
                },
                "status": {
                    "enum": [
                        "BS_Build_Start",
                        "BS_Repair_Start",
                        "BS_Remove_Start",
                        "BS_End"
                    ],
                    "type": "string",
                    "title": "Building Status"
                },
                "petAssignInfo": {
                    "$ref": "#/definitions/model.PetAssignInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"petAssignInfo\""
                },
                "scienceInfo": {
                    "$ref": "#/definitions/model.ScienceInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"scienceInfo\""
                },
                "npcBufferList": {
                    "items": {
                        "$ref": "#/definitions/model.PetBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"npcBufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Building"
        },
        "model.FirstChargeInfo": {
            "properties": {
                "directBuyId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"directBuyId\"  档位"
                },
                "ChargeTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"firstChargeTime\"  首充时间"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "First Charge Info"
        },
        "model.LLKInfo": {
            "properties": {
                "score": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"score\""
                },
                "coin": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"coin\""
                },
                "totalCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"totalCount\"  总局数"
                },
                "winCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"winCount\"  胜局数"
                },
                "loseCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"loseCount\"  胜局数"
                },
                "drawCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"drawCount\"  平局数"
                },
                "fleeCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"fleeCount\"  逃跑数"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "LLK Info"
        },
        "model.Model": {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"id\""
                },
                "direction": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"direction\""
                },
                "x": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"x\""
                },
                "y": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"y\""
                },
                "p": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"p\""
                },
                "params": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"params\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Model"
        },
        "model.PetAssignInfo": {
            "properties": {
                "petUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"petUid\""
                },
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.PetBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Assign Info"
        },
        "model.PetBuffer": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"uid\""
                },
                "bufferId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferId\""
                },
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "bufferTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferTime\"  ! 生效结束时间 0表示永久"
                },
                "bufferSrc": {
                    "enum": [
                        "EBS_Invalid",
                        "EBS_Attr",
                        "EBS_Skill"
                    ],
                    "type": "string",
                    "title": "E Buffer Src"
                },
                "ApplyTargetParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"ApplyTargetParams\""
                },
                "EffectiveConditionID": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveConditionID\""
                },
                "EffectiveParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveParams\""
                },
                "EffectiveProbability": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectiveProbability\""
                },
                "EffectParams": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"EffectParams\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Pet Buffer"
        },
        "model.PlantInfo": {
            "properties": {
                "plantUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"plantUid\""
                },
                "buildUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"buildUid\"  ! 依附的建唯一id, 比如农田的唯一id"
                },
                "buildGlobalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buildGlobalItemId\"  ! 依附建筑的全局id，方便查他的生长速度等"
                },
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\"  ! 植物的全局id"
                },
                "startTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"startTime\"  ! 生效时间"
                },
                "plantOperaList": {
                    "items": {
                        "$ref": "#/definitions/model.PlantOperaInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"plantOperaList\""
                },
                "pruneTimes": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"pruneTimes\"  ! 剪枝 两个状态可以剪"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Plant Info"
        },
        "model.PlantOperaInfo": {
            "properties": {
                "buffGlobalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\"  ! buff等id"
                },
                "startTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"startTime\"  ! 生效时间"
                },
                "endTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"endTime\""
                },
                "operaBaseValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"operaBaseValue\"  ! 如果是0就不用管，如果有值，就是宠物属性计算后的operaValue"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Plant Opera Info"
        },
        "model.ScienceBuffer": {
            "properties": {
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "scienceBuffType": {
                    "enum": [
                        "ESBT_Invalid",
                        "ESBT_ReduceCD",
                        "ESBT_AddOutPut",
                        "ESBT_LandBaseSpeed",
                        "ESBT_LandOutPut",
                        "ESBT_ExpandWarehouseCap"
                    ],
                    "type": "string",
                    "title": "E Science Buff Type"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Science Buffer"
        },
        "model.ScienceInfo": {
            "properties": {
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.ScienceBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Science Info"
        },
        "model.UserBase": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "idNum": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"idNum\"  ! 居民证id"
                },
                "nickName": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"nickName\""
                },
                "sex": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"sex\"  0是女1是男，国际通用"
                },
                "lastChangeNickTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastChangeNickTime\""
                },
                "headFrame": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"headFrame\""
                },
                "lastLoginTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastLoginTime\""
                },
                "rewardList": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"rewardList\"  ! 第0个索引为已领取等级奖励的等级，第1个索引为舒适度等级"
                },
                "llkInfo": {
                    "$ref": "#/definitions/model.LLKInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"llkInfo\""
                },
                "supplyExpireTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"supplyExpireTime\"  ! 月卡过期时间"
                },
                "supplyGetRewardTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"supplyGetRewardTime\"  ! 月卡每日奖励领取时间"
                },
                "firstChargeInfo": {
                    "items": {
                        "$ref": "#/definitions/model.FirstChargeInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"firstChargeInfo\"  ! 首抽信息"
                },
                "is_blue_vip": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"is_blue_vip\""
                },
                "is_blue_year_vip": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"is_blue_year_vip\""
                },
                "blue_vip_level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"blue_vip_level\""
                },
                "is_super_blue_vip": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"is_super_blue_vip\""
                },
                "registerTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"registerTime\""
                },
                "lastLoginOutTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastLoginOutTime\""
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "User Base"
        },
        "model.VillageInfo": {
            "properties": {
                "unlockVillageIdList": {
                    "items": {
                        "enum": [
                            "InvalidVillage",
                            "One",
                            "Two"
                        ]
                    },
                    "type": "array",
                    "title": "Village Id"
                },
                "buildingList": {
                    "items": {
                        "$ref": "#/definitions/model.Building"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"buildingList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Village Info"
        },
        "model.World": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "villageInfo": {
                    "$ref": "#/definitions/model.VillageInfo",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"villageInfo\""
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "World"
        }
    }
}