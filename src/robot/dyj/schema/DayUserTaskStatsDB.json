{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/DayUserTaskStatsDB",
    "definitions": {
        "DayUserTaskStatsDB": {
            "properties": {
                "day": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "totalRegister": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"totalRegister\"  历史注册"
                },
                "registerUser": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"registerUser\"  当日注册"
                },
                "dayTaskCnt": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"dayTaskCnt\"  停留在每个任务的人数(下标0对应任务1)"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Day User Task Stats DB",
            "description": "每天的场景驻留统计"
        }
    }
}