{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Goods",
    "definitions": {
        "Goods": {
            "properties": {
                "goodsItem": {
                    "$ref": "#/definitions/model.CommonItem",
                    "additionalProperties": true,
                    "description": "@inject_tag: bson:\"goodsItem\""
                },
                "loadRewardList": {
                    "items": {
                        "$ref": "#/definitions/model.CommonItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"loadRewardList\"  ! 装车奖励信息"
                },
                "loadFlag": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"loadFlag\"  ! 是否装修货物 1代表装填, 用于单个物品装填后发奖励"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Goods"
        },
        "model.CommonItem": {
            "properties": {
                "itemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemId\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Common Item"
        }
    }
}