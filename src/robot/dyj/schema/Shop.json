{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Shop",
    "definitions": {
        "Shop": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "shopItemList": {
                    "items": {
                        "$ref": "#/definitions/model.ShopItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"shopItemList\""
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Shop"
        },
        "model.ShopItem": {
            "properties": {
                "shopId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"shopId\""
                },
                "buyCnt": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buyCnt\"  ! 已购买次数"
                },
                "lastBuyTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastBuyTime\"  ! 未刷新前购买时间戳"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Shop Item"
        }
    }
}