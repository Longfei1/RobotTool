{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/LLKGameEndEx",
    "definitions": {
        "LLKGameEndEx": {
            "properties": {
                "modelGameEnd": {
                    "$ref": "#/definitions/model.LLKGameEnd",
                    "additionalProperties": true
                },
                "llkReward": {
                    "items": {
                        "$ref": "#/definitions/msg.LLKReward"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "LLK Game End Ex"
        },
        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        },
        "model.LLKGameEnd": {
            "properties": {
                "winUser": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"winUser\""
                },
                "llkInfoList": {
                    "items": {
                        "$ref": "#/definitions/model.LLKInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"llkInfoList\"  这里只是变化量 score就是结算的时候的分"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "LLK Game End"
        },
        "model.LLKInfo": {
            "properties": {
                "score": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"score\""
                },
                "coin": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"coin\""
                },
                "totalCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"totalCount\"  总局数"
                },
                "winCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"winCount\"  胜局数"
                },
                "loseCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"loseCount\"  胜局数"
                },
                "drawCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"drawCount\"  平局数"
                },
                "fleeCount": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"fleeCount\"  逃跑数"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "LLK Info"
        },
        "msg.LLKReward": {
            "properties": {
                "uin": {
                    "type": "string"
                },
                "itemList": {
                    "items": {
                        "$ref": "#/definitions/model.ItemInfo"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "LLK Reward"
        }
    }
}