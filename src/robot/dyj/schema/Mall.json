{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Mall",
    "definitions": {
        "Mall": {
            "properties": {
                "uid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"_id\""
                },
                "mallItemList": {
                    "items": {
                        "$ref": "#/definitions/model.MallItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"mallItemList\""
                },
                "seq": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"seq\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Mall"
        },
        "model.MallItem": {
            "properties": {
                "merchandiseId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"merchandiseId\""
                },
                "buyCnt": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"buyCnt\"  ! 已购买次数"
                },
                "lastBuyTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"lastBuyTime\"  ! 未刷新前购买时间戳"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Mall Item"
        }
    }
}