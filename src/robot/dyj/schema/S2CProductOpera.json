{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/S2CProductOpera",
    "definitions": {
        "S2CProductOpera": {
            "properties": {
                "errCode": {
                    "enum": [
                        "OK",
                        "UNKNOWN_ERROR",
                        "NOT_AUTH",
                        "TOKEN_INVALID",
                        "PLAYER_NOT_FOUND",
                        "REDIRECT",
                        "IDLE_TIMEOUT",
                        "DUP_LOGIN",
                        "PARAM_ERROR",
                        "SERVER_CLOSED",
                        "SYSTEM_ERR",
                        "SCORE_LIMIT",
                        "BUILDING_NOT_FOUND",
                        "BUILDING_STATUS_STARTING",
                        "BUILDING_CAN_NOT_PUT",
                        "BUILDING_CAN_NOT_REMOVE",
                        "BUILDING_CAN_NOT_ADD",
                        "BUILDING_COMFORT_LIMIT",
                        "BUILDING_USER_LVL_LIMIT",
                        "BUILDING_NUM_LIMIT",
                        "BUILDING_COMFORT_ENOUGH",
                        "PLANT_ATTACH_BUILDING_NOT_FOUND",
                        "PLANT_ATTACH_BUILDING_TYPE_ERR",
                        "PLANT_ATTACH_BUILDING_HAVE_ATTACHED",
                        "PLANT_NOT_FOUND",
                        "PLANT_NOT_MATURE",
                        "PLANT_PRUNE_STEP_ERR",
                        "PLANT_PRUNE_TIME_ZERO",
                        "PLANT_GRAFT_STEP_ERR",
                        "PLANT_NOT_FOUND_CAN_DE_BUFFER",
                        "PLANT_ALREADY_DE_BUFFER",
                        "ITEM_NOT_FOUND",
                        "PROP_NOT_FOUND",
                        "PROP_NOT_ENOUGH",
                        "ORDER_NOT_FOUND",
                        "ORDER_SUBMIT_CD",
                        "WAREHOUSE_CAPACITY_ERR",
                        "PRODUCT_NOT_FOUND",
                        "PRODUCT_NOT_END",
                        "CHANGE_NICK_CD_ERR",
                        "SHOP_EXCEED_LIMIT",
                        "TASK_REWARDED",
                        "TASK_COND_NOT_COM",
                        "TASK_PRE_NOT_COM",
                        "FORMULA_NOT_MATCH",
                        "DELIVERY_GOODS_ALREADY_LOAD",
                        "DELIVERY_CARGO_FIN",
                        "DELIVERY_CARGO_NOT_FIN",
                        "DELIVERY_GOODS_ALREADY_REWARD",
                        "DELIVERY_CARGO_STARTED",
                        "XIAO_XIAO_LE_MATCH_TIME_OUT",
                        "XIAO_XIAO_LE_GAME_END",
                        "ACTIVITY_LVL_NOT_MET",
                        "ACTIVITY_NOT_IN_TIME",
                        "EXCHANGE_CODE_INVALID",
                        "EXCHANGE_CODE_EXPIRE",
                        "EXCHANGE_CODE_TIME_NOT_OPEN",
                        "EXCHANGE_CODE_USED",
                        "EXCHANGE_CODE_BATCH_COUNT_LIMIT"
                    ],
                    "type": "string",
                    "title": "STATUS",
                    "description": "protoc --proto_path=./pb --go_out=./pbgo --go_opt=paths=source_relative ./pb/msg.proto错误码"
                },
                "operaCmd": {
                    "enum": [
                        "ProductOperaCmdInvalid",
                        "GetProduct",
                        "ProductStart",
                        "ProductEnd",
                        "UnlockField",
                        "Accelerate"
                    ],
                    "type": "string",
                    "title": "Product Opera Cmd"
                },
                "productInfo": {
                    "$ref": "#/definitions/model.ProductInfo",
                    "additionalProperties": true,
                    "description": "! 全量的，如果领取中间的，中间这个被删除后，后面的会往前"
                },
                "scienceInfo": {
                    "$ref": "#/definitions/model.ScienceInfo",
                    "additionalProperties": true,
                    "description": "科技树信息"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "S 2 C Product Opera"
        },
        "model.ProductInfo": {
            "properties": {
                "buildUid": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"buildUid\""
                },
                "productList": {
                    "items": {
                        "$ref": "#/definitions/model.ProductItem"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"productList\"  ! 长度一定与格子数量相等"
                },
                "slotNum": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"slotNum\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Product Info"
        },
        "model.ProductItem": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "startTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"startTime\"  ! 时间戳(秒)"
                },
                "endTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"endTime\"  ! 时间戳(秒)"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Product Item"
        },
        "model.ScienceBuffer": {
            "properties": {
                "bufferValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"bufferValue\"  ! 放大10W倍"
                },
                "scienceBuffType": {
                    "enum": [
                        "ESBT_Invalid",
                        "ESBT_ReduceCD",
                        "ESBT_AddOutPut",
                        "ESBT_LandBaseSpeed",
                        "ESBT_LandOutPut",
                        "ESBT_ExpandWarehouseCap"
                    ],
                    "type": "string",
                    "title": "E Science Buff Type"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Science Buffer"
        },
        "model.ScienceInfo": {
            "properties": {
                "bufferList": {
                    "items": {
                        "$ref": "#/definitions/model.ScienceBuffer"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"bufferList\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Science Info"
        }
    }
}