{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/PlantOperaInfo",
    "definitions": {
        "PlantOperaInfo": {
            "properties": {
                "buffGlobalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\"  ! buff等id"
                },
                "startTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"startTime\"  ! 生效时间"
                },
                "endTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"endTime\""
                },
                "operaBaseValue": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"operaBaseValue\"  ! 如果是0就不用管，如果有值，就是宠物属性计算后的operaValue"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Plant Opera Info"
        }
    }
}