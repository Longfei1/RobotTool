{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/EmailInfo",
    "definitions": {
        "EmailInfo": {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"id\""
                },
                "type": {
                    "enum": [
                        "EMAIL_SYSTEM"
                    ],
                    "type": "string",
                    "title": "E Email Type"
                },
                "sendTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"sendTime\""
                },
                "readStatus": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"readStatus\""
                },
                "awardList": {
                    "items": {
                        "$ref": "#/definitions/model.ItemInfo"
                    },
                    "type": "array",
                    "description": "@inject_tag: bson:\"awardList\""
                },
                "awardStatus": {
                    "type": "boolean",
                    "description": "@inject_tag: bson:\"awardStatus\""
                },
                "title": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"title\""
                },
                "content": {
                    "type": "string",
                    "description": "@inject_tag: bson:\"content\""
                },
                "expireTime": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"expireTime\"  过期时间戳"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Email Info"
        },
        "model.ItemInfo": {
            "properties": {
                "globalItemId": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"globalItemId\""
                },
                "level": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"level\""
                },
                "itemType": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"itemType\""
                },
                "num": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"num\""
                },
                "invalidTimeStamp": {
                    "type": "integer",
                    "description": "@inject_tag: bson:\"invalidTimeStamp\""
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Item Info"
        }
    }
}